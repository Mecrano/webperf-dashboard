  "description": "Uses pagespeedonline API",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "start": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
