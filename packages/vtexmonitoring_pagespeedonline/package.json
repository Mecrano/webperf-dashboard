    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "@types/ramda": "^0.27.46",
    "@vtex/prettier-config": "^0.3.6",
    "@vtex/tsconfig": "^0.6.0",
    "eslint-config-vtex": "^14.1.1",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "3.8.3"
  }
